##### LM #####
lex
    literal `{ `}
    literal `struct `type
    token id /[A-Za-z_][A-Za-z_0-9]*/
    ignore /[ \t\r\n]+/
end

def attribute
    [`type id]

def struct
    [`struct id `{ attribute* `}]

def program
    [struct*]

int func( P: program )
{
	print( P )
}

int main()
{
    parse P: program[ stdin ]
	func( P )
}

main()

