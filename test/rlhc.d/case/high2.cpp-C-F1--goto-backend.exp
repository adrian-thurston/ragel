int Fsm::execute( const unsigned char *_data, int _len )
{
	const unsigned char *p = _data;
	const unsigned char *pe = _data+_len;
	
	{
		int _trans = 0;
		const unsigned char  *_keys;
		const char  *_inds;
		if ( p == pe  )
		goto _test_eof;
		
		if ( cs == 0  )
		goto _out;
		
		_resume :
		_keys = _Fsm_trans_keys + (cs<<1);
		_inds = _Fsm_indicies + _Fsm_index_offsets[cs];
		if ( ((*( p  ))
		)<= 232 && ((*( p  ))
		)>= 10  )
		{
			int _ic = ( int ) _Fsm_char_class[( int ) ((*( p  ))
			)- 10];
			if ( _ic <= ( int ) (*( _keys+1  ))
			&& _ic >= ( int ) (*( _keys  ))
			)
			_trans = ( int ) (*( _inds + ( int ) (_ic - ( int ) (*( _keys  ))
			) ))
			;
			
			else
			_trans = ( int ) _Fsm_index_defaults[cs];
			
		}
		
		else
		{
			_trans = ( int ) _Fsm_index_defaults[cs];
		}
		
		
		goto _match_cond;
		
		_match_cond :
		cs = ( int ) _Fsm_cond_targs[_trans];
		if ( _Fsm_cond_actions[_trans]== 0  )
		goto _again;
		
		
		switch ( _Fsm_cond_actions[_trans] ) {
			case 1 :
			{
				printf("yes\n");
				
			}
			
			break;
			
		}
		
		_again :
		if ( cs == 0  )
		goto _out;
		
		p+= 1;
		if ( p != pe  )
		goto _resume;
		
		
		_test_eof :
		{}
		
		_out :
		{}
		
	}
	if ( cs == Fsm_error )
	return -1;
	if ( cs >= Fsm_first_final )
	return 1;
	return 0;
}

int Fsm::finish()
{
	if ( cs == Fsm_error )
	return -1;
	if ( cs >= Fsm_first_final )
	return 1;
	return 0;
}

