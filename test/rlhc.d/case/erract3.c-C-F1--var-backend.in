	int erract_finish( struct erract *fsm )
	{
		if ( fsm->cs == erract_error )
		return -1;
		else if ( fsm->cs >= erract_first_final )
		return 1;
		return 0;
	}
	
	#include <stdio.h>
	#include <string.h>
	
	struct erract fsm;
	
	void test( char *buf )
	{
		int len = strlen(buf);
		erract_init( &fsm );
		erract_execute( &fsm, buf, len );
		if ( erract_finish( &fsm ) > 0 )
		printf("ACCEPT\n");
		else
		printf("FAIL\n");
	}
	
