let rpnTests = [
("666\n", 666);
("666 111\n", 111);
("4 3 add\n", 7);
("4 3 +\n", 7);
("4 3 -\n", 1);
("4 3 *\n", 12);
("6 2 /\n", 3);
("0 3 -\n", -3);
("0 3 - abs\n", 3);
(" 2  2 + 3 - \n", 1);
("10 7 3 2 * - +\n", 11);
("abba abba add\n", 1332);
]

let rpnFailTests = [
("\n")
]

let () =
List.iter (fun (s,x) -> test rpn s x) rpnTests;
List.iter (fun s -> error rpn s) rpnFailTests

