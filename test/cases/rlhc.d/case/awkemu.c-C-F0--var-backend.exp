void finishLine()
{
	int i;
	char *pword = blineBuf;
	lineBuf[lineLen] = 0;
	printf("endline(%i): %s\n", words, lineBuf );
	for ( i = 0; i < words; i++ ) {
		printf("  word: %s\n", pword );
		pword += strlen(pword) + 1;
	}
}

static const char  _awkemu_actions [] = { 0, 1, 3, 1, 4, 2, 0, 3, 2, 0, 4, 2, 1, 3, 2, 2, 3, 2, 2, 4, 3, 0, 1, 3, 0 ,  };
