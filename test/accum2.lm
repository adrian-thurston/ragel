context ctx

i: int
j: int
k: int

lex
	ignore /space+/
	literal `* `( `)
	token id /[a-zA-Z_]+/ 
end

def foo [id]

def item 
	[id]
|	[foo]
|	[`( item* `)]
	{
		i = 0
	}

def start 
	[item*]

end # ctx


SP: parser<ctx::start> = new parser<ctx::start>()
SP->ctx = new ctx()
send SP [stdin]
Input: ctx::start = SP->finish()
print( Input )
