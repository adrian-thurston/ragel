##### LM #####
lex
	ignore /space+/
	literal `* `( `)
	token id /[a-zA-Z_]+/
end

def item 
	[id]
|	[`( item* `)]

def start 
	[item*]

parse Input: start[ stdin ]

match Input [ItemList: item*]

for I: item* in repeat( ItemList )
	print( ^I '\n' )

for I: item* in rev_repeat( ItemList )
	print( ^I '\n' )
