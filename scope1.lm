##### LM #####
int f()
{
	i: int = 0
	j: int = 100

	while i < 4 {
		j: int = 200
		if ( i < 1 ) {
			j: int = 300
			print( "i: [$i] j: [$j]\n" )
		}
		elsif ( i < 2 ) {
			j: int = 300
			print( "i: [$i] j: [$j]\n" )
		}
		elsif ( i < 3 ) {
			print( "i: [$i] j: [$j]\n" )
		}
		else {
			print( "i: [$i] j: [$j]\n" )
		}

		i = i + 1
	}

	print( "j: [$j]\n" )
}

f()
##### EXP #####
i: 0 j: 300
i: 1 j: 300
i: 2 j: 200
i: 3 j: 200
j: 100
